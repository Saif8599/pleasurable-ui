<script>
    const leftarm = document.querySelector('.arm-l');
    const harry = document.querySelector('.cartoon');
    
    harry.addEventListener('mouseenter', movearm)
    harry.addEventListener('mouseleave', dontmovearm)

    function movearm() {
        leftarm.classList.add('movearm')
    }

    function dontmovearm() {
        leftarm.classList.remove('movearm')
    }

</script>


<style>
.movearm {
    transform-origin: top right;
    transform-box: fill-box;
    animation: swing-arm 1s ease-in-out infinite;
}

.arm-l {
    transform-origin: top right;
    transform-box: fill-box;
    transform: rotate(-10deg);
}

@keyframes swing-arm {
    0% {
        transform: rotate(-10deg);
    }
    25% {
        transform: rotate(50deg);
    }
    50% {
        transform: rotate(-10deg);
    }
    75% {
        transform: rotate(50deg);
    }
    100% {
        transform: rotate(-10);
    }
}

.torso {
    transform-origin: center;
    transform-box: fill-box;
    animation: move-arm 2s ease-in-out infinite;
}

@keyframes move-arm {
    0% {
        transform: translateY(0px);
    }
    50% {
        transform: translateY(10px); /* or negative to move up first */
    }
    100% {
        transform: translateY(0px);
    }
}
</style>